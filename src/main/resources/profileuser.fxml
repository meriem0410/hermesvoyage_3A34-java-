<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.Controllers.ProfileAdminController">



    <children>
        <Label fx:id="greetingText" layoutX="56.0" layoutY="45.0" prefHeight="67.0" prefWidth="494.0" text="WELCOME">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
        <Button fx:id="profilebtn" layoutX="198.0" layoutY="229.0" onAction="#gotoprofile" prefHeight="18.0" prefWidth="230.0" style="-fx-background-color: #8ac0b6; -fx-background-radius: 30;" text="Profile" textFill="WHITE">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Button>
        <Button fx:id="loginbtn" layoutX="198.0" layoutY="290.0" onAction="#logout" prefHeight="18.0" prefWidth="230.0" style="-fx-background-color: #8ac0b6; -fx-background-radius: 30;" text="Log Out" textFill="WHITE">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Button>
        <Button fx:id="dashboard" layoutX="198.0" layoutY="170.0" onAction="#gotodashboard" prefHeight="18.0" prefWidth="230.0" style="-fx-background-color: #8ac0b6; -fx-background-radius: 30;" text="Dashboard" textFill="WHITE">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
