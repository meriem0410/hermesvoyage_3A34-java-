<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.GestionTransport.Controller.Tickets.AddTicketsController">
   <children>
      <Pane prefHeight="400.0" prefWidth="601.0">
         <children>
            <TextField fx:id="departField" layoutX="57.0" layoutY="62.0" promptText="Depart" />
            <TextField fx:id="arriveField" layoutX="58.0" layoutY="96.0" promptText="Arrive" />
            <TextField fx:id="prixField" layoutX="59.0" layoutY="130.0" promptText="Prix" />
            <TextField fx:id="typeTransportField" layoutX="60.0" layoutY="163.0" promptText="Type de transport" />
            <Button fx:id="addButton" layoutX="58.0" layoutY="215.0" mnemonicParsing="false" onAction="#handleAddButtonAction" text="AddTicket">
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
            </Button>
            <Button fx:id="clearButton" layoutX="162.0" layoutY="216.0" mnemonicParsing="false" onAction="#handleClearButtonAction" text="Clear">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Text layoutX="54.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ajouter Tickets">
               <font>
                  <Font name="System Bold Italic" size="22.0" />
               </font>
            </Text>
            <Button fx:id="RetourButton" layoutX="89.0" layoutY="244.0" mnemonicParsing="false" onAction="#handleRetourButtonAction" text="Retour" />
         </children>
      </Pane>
   </children>
</AnchorPane>
